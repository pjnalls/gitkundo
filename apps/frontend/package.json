{
  "name": "@gitkundo/frontend",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "start": {
        "executor": "@nx/expo:start",
        "options": {}
      },
      "build": {
        "executor": "@nx/expo:prebuild",
        "options": {}
      },
      "sync-deps": {
        "executor": "@nx/expo:sync-deps"
      },
      "test": {
        "executor": "@nx/jest:jest",
        "options": {
          "jestConfig": "apps/frontend/jest.config.ts",
          "passWithNoTests": true
        }
      }
    }
  },
  "scripts": {
    "eas-build-post-install": "cd ../../ && node tools/scripts/eas-build-post-install.mjs . apps/frontend",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/metro-config": "*",
    "@testing-library/react-native": "*",
    "expo": "*",
    "expo-splash-screen": "*",
    "expo-status-bar": "*",
    "expo-system-ui": "*",
    "jest-expo": "*",
    "metro-config": "*",
    "metro-resolver": "*",
    "react": "*",
    "react-native": "*",
    "react-native-svg": "*",
    "react-native-svg-transformer": "*",
    "react-native-web": "*"
  }
}
